{
  "description": "Remove tl search parameter from reddit urls",
  "manifest_version": 2,
  "name": "ReddTL",
  "version": "1.0",
  "homepage_url": "https://github.com/alexpietsch/reddtl",
  "icons": {
    "48": "icons/icon-48.png"
  },
  "permissions": ["webRequest", "webRequestBlocking", "*://*.reddit.com/*"],
  "background": {
    "scripts": ["tl.js"]
  }
}
